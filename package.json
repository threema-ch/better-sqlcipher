{
  "name": "better-sqlcipher",
  "version": "9.6.0-sqlcipher4.5.7-bearssl0.6",
  "description": "better-sqlite3 built with SQLCipher",
  "author": "Threema GmbH <opensource@threema.ch>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "binding.gyp",
    "src/*.[ch]pp",
    "lib/**",
    "deps/**"
  ],
  "devDependencies": {
    "chai": "^4.3.8",
    "cli-color": "^2.0.3",
    "fs-extra": "^11.1.1",
    "mocha": "^10.2.0",
    "tar": "^6.1.13"
  },
  "scripts": {
    "install": "node-gyp rebuild --release -j max",
    "build-release": "node-gyp rebuild --release -j max",
    "build-debug": "node-gyp rebuild --debug -j max",
    "rebuild-release": "npm run lzz && npm run build-release",
    "rebuild-debug": "npm run lzz && npm run build-debug",
    "test": "mocha --exit --slow=75 --timeout=5000",
    "benchmark": "node benchmark",
    "download": "(cd deps && bash ./download.sh)",
    "lzz": "lzz -hx hpp -sx cpp -k BETTER_SQLITE3 -d -hl -sl -e ./src/better_sqlite3.lzz"
  },
  "license": "MIT",
  "keywords": [
    "sql",
    "sqlite",
    "sqlite3",
    "sqlcipher",
    "transactions",
    "user-defined functions",
    "aggregate functions",
    "window functions",
    "database"
  ]
}
